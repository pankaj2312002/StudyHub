{
    "version": 2,
    "builds": [
      {
        "src": "Frontend/package.json",
        "use": "@vercel/react"
      },
      {
        "src": "Backend/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Frontend/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "Backend/$1"
      }
    ]
  }
  